{
  "name": "anwar/attendance-sync",
  "description": "Kiosk attendance sync package for Laravel 10",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Anwar\\AttendanceSync\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Anwar\\AttendanceSync\\Providers\\AttendanceSyncServiceProvider"
      ]
    }
  },
  "replace": {
    "kiosk/attendance-sync": "self.version"
  },
  "require": {
    "php": "^7.3 || ^8.0",
    "illuminate/support": "^8.0 || ^9.0 || ^10.0",
    "illuminate/http": "^8.0 || ^9.0 || ^10.0",
    "illuminate/database": "^8.0 || ^9.0 || ^10.0",
    "illuminate/validation": "^8.0 || ^9.0 || ^10.0"
  }
}
